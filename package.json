{
  "name": "wzl-twitter-media-downloader",
  "version": "1.0.0",
  "description": "Small Electron app which helps you download all your faved images from Twitter",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "yarn build:mac && yarn build:win && yarn build:linux",
    "build:mac": "electron-packager . --platform=darwin --arch=x64,arm64 --out=../wzl-twitter-builds/out/macOS --overwrite",
    "build:win": "electron-packager . --platform=win32 --arch=x64 --out=../wzl-twitter-builds/out/windows --overwrite",
    "build:linux": "electron-packager . --platform=linux --arch=x64 --out=../wzl-twitter-builds/out/linux --overwrite",
    "test": "test"
  },
  "repository": "git+https://github.com/mg-wzl/wzl-twitter-scraper.git",
  "keywords": [
    "twitter",
    "scraper"
  ],
  "author": "mg-wzl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mg-wzl/wzl-twitter-scraper/issues"
  },
  "homepage": "https://github.com/mg-wzl/wzl-twitter-scraper#readme",
  "private": false,
  "devDependencies": {
    "electron": "^21.3.0",
    "electron-packager": "^17.1.1"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "custom-electron-titlebar": "^4.1.3",
    "dayjs": "^1.11.6",
    "electron-context-menu": "^3.6.0",
    "electron-dl": "^3.5.0",
    "jsdom": "^20.0.2"
  }
}
